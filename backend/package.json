{
  "name": "modish-log-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "test": "jest",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate": "npm run typeorm migration:generate -- -d ./src/data-source.ts",
