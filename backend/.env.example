# This file contains example environment variables for the backend service
# Copy this file to .env and replace with actual values

# PostgreSQL Database Connection
DATABASE_URL=postgresql://postgres:password@localhost:5432/modish-log

# JWT Authentication
JWT_SECRET=your-secret-key-here
JWT_EXPIRATION=1h

# Server Configuration
PORT=3001
NODE_ENV=development

# Logging
LOG_LEVEL=info
